{
    "presets": [
        [
            "@babel/preset-env",
            {
                "modules": "cjs"
            }
        ],
        "@babel/preset-typescript"
    ],
    "plugins": [
        "babel-plugin-add-import-extension",
        [
            "babel-plugin-transform-import-extension",
            {
                "extMapping": {
                    ".ts": ".js"
                }
            }
        ]
    ]
}